{
  "expectation_suite_name": "cross_daily_suite",
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_set",
      "kwargs": {
        "column_set": [
          "fecha",
          "numero",
          "score_cruzado",
          "oportunidades_total",
          "activaciones_total",
          "combinaciones_activas",
          "detalle_cruzado"
        ]
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "fecha" }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": { "column": "numero", "regex": "^[0-9]{2}$" }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "score_cruzado", "min_value": 0.0, "max_value": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "oportunidades_total", "min_value": 0, "mostly": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "activaciones_total", "min_value": 0, "mostly": 1.0 }
    },
    {
      "expectation_type": "expect_column_pair_values_a_to_be_greater_than_b",
      "kwargs": {
        "column_A": "oportunidades_total",
        "column_B": "activaciones_total",
        "or_equal": true,
        "ignore_row_if": "either_value_is_missing"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "combinaciones_activas", "min_value": 0, "mostly": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "detalle_cruzado" }
    }
  ],
  "meta": {
    "notes": "Valida columnas principales del score cruzado diario y garantiza que activaciones â‰¤ oportunidades."
  }
}
