{
  "expectation_suite_name": "fusion_daily_suite",
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_set",
      "kwargs": {
        "column_set": [
          "fecha",
          "numero",
          "score_cruzado",
          "score_estructural",
          "score_derivado",
          "score_total",
          "prob",
          "tipo_convergencia",
          "detalles"
        ]
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "fecha" }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": { "column": "numero", "regex": "^[0-9]{2}$" }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "prob", "min_value": 0.0, "max_value": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "score_cruzado", "min_value": 0.0, "mostly": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "score_estructural", "min_value": 0.0, "mostly": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": { "column": "score_derivado", "min_value": 0.0, "mostly": 1.0 }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "score_total" }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "tipo_convergencia",
        "value_set": [
          "C+E+D",
          "C+E",
          "C+D",
          "E+D",
          "C",
          "E",
          "D",
          "Sin Activaci√≥n",
          "Sin Activaci\u00f3n",
          "Sin Activacion"
        ]
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": { "column": "detalles" }
    }
  ],
  "meta": {
    "notes": "Revisa columnas y dominios del dataset fusion_daily."
  }
}
